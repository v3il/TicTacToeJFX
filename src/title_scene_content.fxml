<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="300.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kit.tictactoe.controllers.TitleScene">
   <children>
      <Label text="Java FX TicTacToe" textAlignment="CENTER" textFill="#b20000">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Label text="Нік першого гравця">
         <VBox.margin>
            <Insets bottom="10.0" top="50.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="firstPlayerNickname" onKeyReleased="#validateNicknames" text="Гравець 1" />
      <Label text="Нік другого гравця">
         <VBox.margin>
            <Insets bottom="10.0" top="20.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="secondPlayerNickname" onKeyReleased="#validateNicknames" text="Гравець 2" />
      <Button fx:id="startGameBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#startGame" text="Почати гру" VBox.vgrow="NEVER">
         <VBox.margin>
            <Insets top="40.0" />
         </VBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
